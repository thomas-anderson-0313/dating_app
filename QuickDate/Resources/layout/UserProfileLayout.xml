<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <androidx.core.widget.NestedScrollView
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        android:layout_width="match_parent"
        android:layout_marginTop="0dp"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:background="?attr/colorFill"
            android:paddingBottom="20dp">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="330dp"
                android:layout_alignParentTop="true"
                android:orientation="vertical">
                <ImageView
                    android:contentDescription="likeprofile"
                    android:id="@+id/UserImage"
                    android:scaleType="centerCrop"
                    android:adjustViewBounds="true"
                    android:src="@drawable/ic_photo_camera"
                    android:layout_width="match_parent"
                    android:layout_height="330dp" />
                <TextView
                    android:id="@+id/back"
                    android:text="IC"
                    android:textColor="@color/textIcon"
                    android:textSize="30sp"
                    android:layout_height="100dp"
                    android:layout_width="100dp"
                    android:layout_gravity="start"
                    android:layout_marginStart="11dp"
                    android:paddingBottom="16dp"
                    android:paddingTop="15dp" />
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:layout_alignParentBottom="true"
                    android:orientation="vertical"
                    android:layout_marginTop="0dp">
                    <View
                        android:background="@drawable/rounded_corner_from_top_shape"
                        android:layout_alignParentBottom="true"
                        android:layout_width="match_parent"
                        android:layout_height="40dp" />
                    <FrameLayout
                        android:clickable="true"
                        android:focusable="true"
                        android:layout_width="45dp"
                        android:layout_height="45dp"
                        android:layout_marginBottom="0dp"
                        android:foregroundGravity="center_horizontal"
                        android:layout_centerInParent="true"
                        android:layout_centerHorizontal="true"
                        android:layout_alignParentEnd="true"
                        android:id="@+id/ActionButton1"
                        android:layout_marginEnd="17dp"
                        android:layout_marginTop="0dp"
                        android:background="@drawable/circlegradient">
                        <ImageView
                            android:id="@+id/Image"
                            android:layout_gravity="center"
                            android:layout_width="19dp"
                            android:layout_height="19dp"
                            android:tint="@color/white"
                            android:src="@drawable/icon_gift_vector"
                            android:gravity="center" />
                    </FrameLayout>
                    <FrameLayout
                        android:id="@+id/ActionButton2"
                        android:layout_toStartOf="@+id/ActionButton1"
                        android:clickable="true"
                        android:focusable="true"
                        android:layout_width="45dp"
                        android:layout_height="45dp"
                        android:layout_centerInParent="true"
                        android:layout_marginBottom="0dp"
                        android:foregroundGravity="center_horizontal"
                        android:layout_centerHorizontal="true"
                        android:layout_marginEnd="12dp"
                        android:layout_marginTop="0dp"
                        android:background="@drawable/circlegradient4">
                        <ImageView
                            android:id="@+id/iconFavorite"
                            android:layout_gravity="center"
                            android:layout_width="19dp"
                            android:layout_height="19dp"
                            android:tint="@color/white"
                            android:src="@drawable/icon_fav_post_vector"
                            android:gravity="center" />
                    </FrameLayout>
                    <FrameLayout
                        android:id="@+id/ActionButton3"
                        android:layout_toStartOf="@+id/ActionButton2"
                        android:clickable="true"
                        android:focusable="true"
                        android:layout_width="45dp"
                        android:layout_height="45dp" 
                        android:layout_centerInParent="true"
                        android:layout_marginBottom="0dp"
                        android:foregroundGravity="center_horizontal"
                        android:layout_centerHorizontal="true"
                        android:layout_marginEnd="12dp"
                        android:layout_marginTop="0dp"
                        android:background="@drawable/circlegradient2">
                        <ImageView
                            android:id="@+id/iconFriendRequests"
                            android:layout_gravity="center"
                            android:layout_width="19dp"
                            android:layout_height="19dp"
                            android:tint="@color/white"
                            android:src="@drawable/icon_friendplus_vector"
                            android:gravity="center" />
                    </FrameLayout>
                </RelativeLayout>
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/Username"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    android:drawablePadding="5dp"
                    android:layout_marginStart="25dp" />
                <View
                    android:id="@+id/iconOnline"
                    android:layout_above="@+id/LastSeen"
                    android:layout_marginTop="10dp"
                    android:layout_marginStart="10dp"
                    android:layout_toEndOf="@+id/Username"
                    android:backgroundTint="#30db3c"
                    android:background="@drawable/circlegradient"
                    android:layout_width="10dp"
                    android:layout_height="10dp" />
                <ImageView
                    android:layout_marginTop="6dp"
                    android:layout_marginStart="10dp"
                    android:layout_toEndOf="@+id/iconOnline"
                    android:id="@+id/iconMore"
                    android:layout_gravity="center"
                    android:layout_width="19dp"
                    android:layout_height="19dp"
                    android:tint="#999"
                    android:src="@drawable/icon_more_dots_vector"
                    android:gravity="center" />
                <ImageView
                    android:layout_below="@+id/Username"
                    android:translationZ="3dp"
                    android:layout_marginTop="3dp"
                    android:layout_marginEnd="30dp"
                    android:layout_alignParentEnd="true"
                    android:layout_width="21dp"
                    android:id="@+id/iconQuote"
                    android:tint="@color/accent"
                    android:src="@drawable/icon_quote_vector"
                    android:layout_height="21dp" />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="13dp"
                    android:layout_below="@+id/Username"
                    android:id="@+id/AboutTextcardview"
                    android:layout_marginStart="25dp"
                    android:background="@drawable/Shape_rounded_with_shadow"
                    android:layout_marginEnd="25dp">
                    <TextView
                        android:id="@id/AboutText"
                        android:layout_below="@+id/Username"
                        android:drawableTint="#595959"
                        android:textStyle="normal"
                        android:layout_marginTop="0dp"
                        android:layout_marginBottom="0dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:textAlignment="center"
                        android:paddingStart="5dp"
                        android:paddingEnd="5dp"
                        android:paddingTop="11dp"
                        android:paddingBottom="11dp"
                        android:layout_marginStart="1dp"
                        android:textColor="#595959"
                        android:textSize="13sp" />
                </LinearLayout>
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/Recyler"
                    android:layout_below="@+id/AboutTextcardview"
                    android:layout_marginTop="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="105dp"
                    android:scrollbars="none" 
                    android:layout_marginStart="22dp"
                    android:layout_marginEnd="25dp" />
                <TextView
                    android:id="@+id/Description"
                    android:layout_below="@+id/Recyler"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center|start"
                    android:layout_marginTop="10dp"
                    android:paddingStart="15dp"
                    android:paddingEnd="24dp"
                    android:paddingTop="15dp"
                    android:layout_marginStart="10dp"
                    android:textSize="13sp" />
            </RelativeLayout>
            <RelativeLayout
                android:paddingTop="5dp"
                android:layout_marginTop="9dp"
                android:layout_marginBottom="20dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    style="@style/CustomTextdrawableStyle"
                    android:drawableStart="@drawable/icon_place_vector"
                    android:id="@+id/LocationTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="0dp"
                    android:textSize="13sp"
                    android:drawableTint="#F44336"
                    android:layout_marginTop="6dp"
                    android:layout_marginStart="10dp" />
                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="130dp" 
                    android:layout_below="@+id/LocationTextView"
                    android:id="@+id/mapCardView"
                    android:layout_marginStart="25dp"
                    android:layout_marginTop="4dp"
                    android:layout_marginEnd="25dp"
                    app:cardBackgroundColor="?attr/colorFill"
                    app:cardCornerRadius="20dp"
                    app:cardElevation="0.5dp">
	                <FrameLayout
		                android:id="@+id/map" 
		                android:layout_width="match_parent"
		                android:layout_height="130dp" /> 
                </androidx.cardview.widget.CardView>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/workAndEducationLayout"
                android:paddingTop="5dp"
                android:layout_marginTop="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/workAndEducationHead"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/Lbl_WorkAndEducation"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    android:layout_marginStart="25dp" />
                <TextView
                    android:id="@+id/DescriptionworkSection"
                    android:layout_below="@+id/workAndEducationHead"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center|start"
                    android:layout_marginTop="2dp"
                    android:paddingStart="15dp"
                    android:paddingEnd="29dp"
                    android:paddingTop="3dp"
                    android:layout_marginStart="10dp"
                    android:textColor="#595959"
                    android:textSize="13sp" />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/intersetLayout"
                android:paddingTop="5dp"
                android:layout_marginTop="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/IntersetsHead"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/Lbl_Interests"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    android:layout_marginStart="25dp" />
                <TextView
                    android:id="@+id/DescriptionIntersetsSection"
                    android:layout_below="@+id/IntersetsHead"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center|start"
                    android:layout_marginTop="2dp"
                    android:paddingStart="15dp"
                    android:paddingEnd="31dp"
                    android:paddingTop="3dp"
                    android:layout_marginStart="10dp"
                    android:textColor="#595959"
                    android:textSize="15sp" />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/languagesLayout"
                android:paddingTop="5dp"
                android:layout_marginTop="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/LanguagesHead"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/Lbl_Languages"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    android:layout_marginStart="25dp" />
                <TextView
                    android:id="@+id/DescriptionLanguagesSection"
                    android:layout_below="@+id/LanguagesHead"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center|start"
                    android:layout_marginTop="2dp"
                    android:paddingStart="15dp"
                    android:paddingEnd="29dp"
                    android:paddingTop="3dp"
                    android:layout_marginStart="10dp"
                    android:textColor="#595959"
                    android:textSize="15sp" />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/socialinfolayout"
                android:paddingTop="5dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="9dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/SocialinfoHead"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/Lbl_SocialInfo"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    android:layout_marginStart="25dp" />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="2dp"
                    android:paddingStart="15dp"
                    android:paddingEnd="29dp"
                    android:paddingTop="3dp"
                    android:layout_below="@+id/SocialinfoHead"
                    android:gravity="start">
                    <Button
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:id="@+id/social1"
                        android:visibility="gone"
                        android:textSize="14sp"
                        android:layout_marginStart="20dp"
                        android:layout_marginEnd="5dp"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/google_button"
                        android:tag="false"
                        android:paddingTop="1dp"
                        android:paddingStart="2dp"
                        android:paddingEnd="2dp"
                        android:textAlignment="center"
                        android:textAllCaps="false"
                        android:text="G"
                        android:textColor="#DB4437" />
                    <Button
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:id="@+id/social2"
                        android:textSize="14sp"
                        android:visibility="gone"
                        android:layout_toEndOf="@id/social1"
                        android:layout_marginStart="6dp"
                        android:layout_marginEnd="5dp"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/facebook_button"
                        android:tag="false"
                        android:paddingTop="1dp"
                        android:paddingStart="2dp"
                        android:paddingEnd="2dp"
                        android:textAlignment="center"
                        android:textAllCaps="false"
                        android:text="F"
                        android:textColor="#3B5998" />
                    <Button
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:id="@+id/social3"
                        android:textSize="14sp"
                        android:visibility="gone"
                        android:layout_toEndOf="@id/social2"
                        android:layout_marginStart="6dp"
                        android:layout_marginEnd="6dp"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/twitter_button"
                        android:tag="false"
                        android:paddingTop="1dp"
                        android:paddingStart="2dp"
                        android:paddingEnd="2dp"
                        android:textAlignment="center"
                        android:textAllCaps="false"
                        android:text="T"
                        android:textColor="#1DA1F2" />
                    <Button
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:id="@+id/social4"
                        android:textSize="14sp"
                        android:visibility="gone"
                        android:layout_toEndOf="@id/social3"
                        android:layout_marginStart="6dp"
                        android:layout_marginEnd="6dp"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/linkdin_button"
                        android:tag="false"
                        android:paddingTop="1dp"
                        android:paddingStart="2dp"
                        android:paddingEnd="2dp"
                        android:textAlignment="center"
                        android:textAllCaps="false"
                        android:text="LI"
                        android:textColor="#0077B5" />
                    <Button
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:id="@+id/social5"
                        android:textSize="14sp"
                        android:visibility="gone"
                        android:layout_toEndOf="@id/social4"
                        android:layout_marginStart="6dp"
                        android:layout_marginEnd="6dp"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/Instagram_button"
                        android:tag="false"
                        android:paddingTop="1dp"
                        android:paddingStart="2dp"
                        android:paddingEnd="2dp"
                        android:textAlignment="center"
                        android:textAllCaps="false"
                        android:text="In"
                        android:textColor="#e4405f" />
                    <Button
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:id="@+id/website"
                        android:textSize="14sp"
                        android:visibility="gone"
                        android:layout_toEndOf="@id/social5"
                        android:layout_marginStart="6dp"
                        android:layout_marginEnd="6dp"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/follow_button_profile_friends"
                        android:tag="false"
                        android:paddingTop="1dp"
                        android:paddingStart="2dp"
                        android:paddingEnd="2dp"
                        android:textAlignment="center"
                        android:textAllCaps="false"
                        android:text="W"
                        android:textColor="@color/accent" />
                </LinearLayout>
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="90dp" />
        </LinearLayout>
    </androidx.core.widget.NestedScrollView>
    <RelativeLayout
        android:id="@+id/footerButtonSection"
        android:layout_alignParentBottom="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@xml/framealfacolor"
        android:paddingBottom="25dp"
        android:gravity="center">
        <FrameLayout
            android:id="@+id/Chatbutton1"
            android:clickable="true"
            android:focusable="true"
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:layout_centerInParent="true"
            android:layout_marginBottom="0dp"
            android:foregroundGravity="center_horizontal"
            android:layout_marginEnd="10dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/circlegradient3">
            <ImageView
                android:id="@+id/Image"
                android:layout_gravity="center"
                android:layout_width="19dp"
                android:layout_height="19dp"
                android:tint="@color/white"
                android:src="@drawable/icon_message_vector"
                android:gravity="center" />
        </FrameLayout>
        <at.markushi.ui.CircleButton
            android:id="@+id/likebutton2"
            android:layout_width="75dip"
            android:layout_height="75dip"
            app:cb_color="#f44336"
            app:cb_pressedRingWidth="8dip"
            android:src="@drawable/ic_action_like"
            android:layout_alignParentBottom="false"
            android:layout_alignParentEnd="false"
            android:layout_marginStart="5dp"
            android:layout_marginTop="10dp"
            android:tint="@color/textIcon"
            android:elevation="10dp"
            android:layout_toEndOf="@+id/Chatbutton1"
            android:fadeScrollbars="false"
            android:clickable="false"
            android:layout_centerHorizontal="true" />
        <at.markushi.ui.CircleButton
            android:id="@+id/closebutton1"
            android:layout_width="75dip"
            android:layout_height="75dip"
            app:cb_color="@color/primary"
            android:elevation="10dp"
            app:cb_pressedRingWidth="8dip"
            android:src="@drawable/ic_close_padded"
            android:layout_marginEnd="15dp"
            android:layout_marginTop="10dp"
            android:tint="@color/textIcon"
            android:layout_toStartOf="@+id/Chatbutton1"
            android:foregroundGravity="right" />
    </RelativeLayout>
</RelativeLayout>