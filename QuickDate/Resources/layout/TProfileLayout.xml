<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_gravity="top"
    android:layout_alignParentTop="true"
    android:gravity="top"
    android:background="?attr/colorPrimary"
    android:orientation="vertical"
    android:paddingBottom="50dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_gravity="top"
        android:layout_alignParentTop="true"
        android:gravity="top"
        android:paddingBottom="0dp"
        android:background="?attr/colorPrimary"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="top"
            android:layout_alignParentTop="true"
            android:gravity="top">
            <View
                android:layout_gravity="top"
                android:layout_alignParentTop="true"
                android:gravity="top"
                android:layout_width="match_parent"
                android:layout_height="550dp"
                android:background="@drawable/shape_halfcircle" />
            <LinearLayout
                android:id="@+id/headerSection"
                android:weightSum="6"
                android:layout_centerHorizontal="true"
                android:gravity="center_horizontal"
                android:foregroundGravity="center_horizontal"
                android:orientation="horizontal"
                android:layout_marginStart="15dp"
                android:layout_marginEnd="15dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <RelativeLayout
                    android:orientation="horizontal"
                    android:id="@+id/walletSection"
                    android:layout_width="wrap_content"
                    android:clickable="true"
                    android:layout_marginTop="4dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:background="@drawable/shape_radius_btn"
                    android:layout_height="30dp">
                    <ImageView
                        android:id="@+id/coinImageWallet"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:alpha=".90"
                        android:src="@drawable/dollar"
                        android:layout_marginEnd="5dp"
                        android:layout_marginTop="0dp"
                        android:layout_alignParentStart="true"
                        android:layout_marginStart="3dp"
                        android:scaleType="centerCrop" />
                    <TextView
                        android:layout_toEndOf="@+id/coinImageWallet"
                        android:id="@+id/walletnumber"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/textIcon"
                        android:layout_marginEnd="2dp"
                        android:text="------"
                        android:layout_marginTop="1dp"
                        android:layout_marginStart="3dp"
                        android:textSize="12sp"
                        android:layout_marginBottom="0dp" />
                </RelativeLayout>
                <RelativeLayout
                    android:orientation="horizontal"
                    android:id="@+id/popularitySection"
                    android:layout_alignParentTop="true"
                    android:layout_width="wrap_content"
                    android:layout_marginStart="12dp"
                    android:layout_weight="1"
                    android:layout_marginTop="4dp"
                    android:clickable="true"
                    android:layout_centerHorizontal="true"
                    android:gravity="center"
                    android:background="@drawable/shape_radius_btn"
                    android:layout_height="30dp">
                    <ImageView
                        android:id="@+id/coinImagePopularity"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:alpha=".90"
                        android:src="@drawable/speedometer"
                        android:layout_marginEnd="5dp"
                        android:layout_marginTop="0dp"
                        android:layout_alignParentStart="true"
                        android:layout_marginStart="2dp"
                        android:scaleType="centerCrop" />
                    <TextView
                        android:id="@+id/popularityText"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/textIcon"
                        android:layout_marginEnd="2dp"
                        android:text="@string/Lbl_VeryLow"
                        android:layout_marginTop="1dp"
                        android:layout_toEndOf="@+id/coinImagePopularity"
                        android:layout_marginStart="3dp"
                        android:textSize="12sp"
                        android:layout_marginBottom="0dp" />
                </RelativeLayout>
                <RelativeLayout
                    android:orientation="horizontal"
                    android:id="@+id/upgradeSection"
                    android:layout_width="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="4dp"
                    android:clickable="true"
                    android:gravity="center"
                    android:background="@drawable/shape_radius_btn"
                    android:layout_height="30dp">
                    <ImageView
                        android:id="@+id/coinImageUpgrade"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:alpha=".90"
                        android:src="@drawable/diamond"
                        android:layout_marginEnd="5dp"
                        android:layout_marginTop="0dp"
                        android:layout_alignParentStart="true"
                        android:layout_marginStart="2dp"
                        android:scaleType="centerCrop" />
                    <TextView
                        android:id="@+id/upgradeText"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/textIcon"
                        android:layout_marginEnd="2dp"
                        android:layout_marginTop="1dp"
                        android:layout_toEndOf="@+id/coinImageUpgrade"
                        android:text="@string/Lbl_Upgrade"
                        android:layout_marginStart="3dp"
                        android:textSize="12sp"
                        android:layout_marginBottom="0dp" />
                </RelativeLayout>
            </LinearLayout>
            <RelativeLayout
                android:id="@+id/imagelayout"
                android:paddingTop="3dp"
                android:layout_below="@+id/headtitle"
                android:layout_marginTop="2dp"
                android:layout_centerHorizontal="true"
                android:layout_width="match_parent"
                android:gravity="center"
                android:layout_height="wrap_content">
                <ImageView
                    android:id="@+id/Dialog_heart1"
                    android:layout_width="29dp"
                    android:layout_height="29dp"
                    android:alpha=".80"
                    android:rotation="-15"
                    android:src="@drawable/Dialog_heart"
                    android:layout_marginEnd="0dp"
                    android:layout_marginTop="60dp"
                    android:layout_toStartOf="@+id/Iconimage"
                    android:layout_marginStart="50dp"
                    android:scaleType="centerCrop" />
                <ImageView
                    android:id="@+id/Iconimage2"
                    android:layout_width="105dp"
                    android:layout_height="105dp"
                    android:layout_centerHorizontal="true"
                    android:src="@drawable/no_profile_image_circle"
                    android:layout_marginEnd="0dp"
                    android:layout_marginTop="55dp"
                    android:layout_marginStart="40dp"
                    android:scaleType="centerCrop" />
                <ImageView
                    android:id="@+id/Dialog_heart2"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    android:alpha=".75"
                    android:rotation="3"
                    android:src="@drawable/Dialog_heart"
                    android:layout_marginEnd="0dp" 
                    android:layout_marginTop="50dp"
                    android:layout_toEndOf="@+id/Iconimage2" 
                    android:layout_marginStart="15dp"
                    android:scaleType="centerCrop" />
                <ImageView
                    android:id="@+id/Dialog_heart3"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:alpha=".75"
                    android:src="@drawable/Dialog_heart"
                    android:layout_marginEnd="0dp"
                    android:layout_marginTop="55dp"
                    android:tint="@color/textIcon"
                    android:layout_alignBottom="@+id/Iconimage2"
                    android:layout_marginStart="-16dp"
                    android:scaleType="centerCrop" />
                <ImageView
                    android:id="@+id/Dialog_heart4"
                    android:layout_width="39dp"
                    android:layout_height="39dp"
                    android:alpha=".75"
                    android:src="@drawable/Dialog_heart"
                    android:layout_marginEnd="30dp"
                    android:layout_marginTop="55dp"
                    android:rotation="15"
                    android:layout_alignParentEnd="true"
                    android:layout_above="@+id/username"
                    android:layout_marginStart="12dp"
                    android:scaleType="centerCrop" />
                <TextView
                    android:layout_below="@+id/Iconimage2"
                    android:id="@+id/username"
                    android:text="------"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/textIcon"
                    android:gravity="center"
                    android:drawablePadding="5dp"
                    android:drawableTint="#6fa7e1"
                    android:layout_marginEnd="40dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginStart="45dp"
                    android:layout_centerHorizontal="true"
                    android:textSize="18sp"
                    android:textStyle="bold"
                    android:layout_marginBottom="15dp" />
                <LinearLayout
                    android:layout_centerHorizontal="true"
                    android:layout_below="@+id/username"
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <RelativeLayout
                        android:layout_below="@+id/username"
                        android:orientation="horizontal"
                        android:id="@+id/VisitsSection"
                        android:layout_width="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="3dp"
                        android:clickable="true"
                        android:backgroundTint="#80000000"
                        android:gravity="center"
                        android:background="@drawable/shape_radius_btn"
                        android:layout_height="32dp">
                        <ImageView
                            android:id="@+id/ImageLikes"
                            android:layout_width="18dp"
                            android:layout_height="18dp"
                            android:alpha=".90"
                            android:src="@drawable/heart"
                            android:layout_marginEnd="5dp"
                            android:layout_marginTop="0dp"
                            android:layout_centerVertical="true"
                            android:layout_alignParentStart="true"
                            android:layout_marginStart="10dp"
                            android:scaleType="centerCrop" />
                        <TextView
                            android:id="@+id/LikesText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/textIcon"
                            android:layout_marginEnd="12dp"
                            android:layout_centerVertical="true"
                            android:layout_toEndOf="@+id/ImageLikes"
                            android:text="0 Likes"
                            android:layout_marginStart="5dp"
                            android:textSize="12sp"
                            android:layout_marginBottom="0dp" />
                    </RelativeLayout>
                    <RelativeLayout
                        android:layout_below="@+id/username"
                        android:orientation="horizontal"
                        android:id="@+id/LikesSection"
                        android:layout_width="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="4dp"
                        android:clickable="true"
                        android:gravity="center"
                        android:backgroundTint="#80000000"
                        android:background="@drawable/shape_radius_btn"
                        android:layout_height="32dp">
                        <ImageView
                            android:id="@+id/ImageVisits"
                            android:layout_width="18dp"
                            android:layout_height="18dp"
                            android:alpha=".90"
                            android:tint="#ffff"
                            android:src="@drawable/icon_eyes_filled_vector"
                            android:layout_marginEnd="5dp"
                            android:layout_marginTop="0dp"
                            android:layout_centerVertical="true"
                            android:layout_alignParentStart="true"
                            android:layout_marginStart="10dp"
                            android:scaleType="centerCrop" />
                        <TextView
                            android:id="@+id/VisitsText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/textIcon"
                            android:layout_marginEnd="12dp"
                            android:layout_centerVertical="true"
                            android:layout_toEndOf="@+id/ImageVisits"
                            android:text="0 Visits"
                            android:layout_marginStart="5dp"
                            android:textSize="12sp"
                            android:layout_marginBottom="0dp" />
                    </RelativeLayout>
	                <RelativeLayout
                        android:layout_below="@+id/username"
                        android:orientation="horizontal"
                        android:id="@+id/ShareSection"
                        android:layout_width="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="4dp"
						android:padding="5dp"
                        android:clickable="true"
                        android:gravity="center"
                        android:backgroundTint="#80000000"
                        android:background="@drawable/shape_radius_btn"
                        android:layout_height="32dp"> 
		                <ImageView
                            android:id="@+id/ImageShare"
                            android:layout_width="18dp"
                            android:layout_height="18dp"
                            android:alpha=".90"
                            android:tint="#ffff" 
                            android:src="@drawable/icon_share_vector"   
                            android:layout_centerVertical="true"
                            android:layout_alignParentStart="true" 
                            android:scaleType="centerCrop" />  
                    </RelativeLayout>
                </LinearLayout>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/circleLayouts"
                android:layout_below="@+id/imagelayout"
                android:layout_marginTop="-22dp"
                android:layout_centerInParent="true"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <me.itangqi.waveloadingview.WaveLoadingView
                    android:id="@+id/waveLoadingView"
                    android:layout_width="65dp"
                    android:layout_height="65dp"
                    android:layout_marginTop="38dp"
                    android:layout_centerHorizontal="true"
                    app:wlv_borderColor="@color/primary"
                    app:wlv_borderWidth="3dp"
                    app:wlv_progressValue="0"
                    app:wlv_shapeType="circle"
                    app:wlv_round_rectangle="true"
                    app:wlv_triangle_direction="north"
                    app:wlv_waveAmplitude="50"
                    app:wlv_waveColor="@color/accent" />
                <FrameLayout
                    android:id="@+id/BoostButton"
                    android:clickable="true"
                    android:focusable="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="38dp"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:gravity="center"
                    android:layout_marginEnd="10dp"
                    android:background="@drawable/circlegradient5">
                    <ImageView
                        android:id="@+id/Image"
                        android:layout_gravity="center"
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:tint="@color/white"
                        android:src="@drawable/icon_flash_vector"
                        android:gravity="center" />
                </FrameLayout>
                <TextView
                    android:layout_below="@+id/BoostButton"
                    android:id="@+id/tv_Boost"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/Lbl_BoostMe"
                    android:textStyle="bold"
                    android:gravity="center"
                    android:layout_marginTop="4dp"
                    android:layout_centerHorizontal="true"
                    android:textSize="12sp" />
                <RelativeLayout
                    android:layout_marginEnd="15dp"
                    android:layout_marginTop="10dp"
                    android:layout_alignParentEnd="true"
                    android:gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <FrameLayout
                        android:id="@+id/EditButton"
                        android:clickable="true"
                        android:focusable="true"
                        android:layout_width="55dp"
                        android:layout_height="55dp"
                        android:gravity="center"
                        android:layout_centerHorizontal="true"
                        android:background="@drawable/circlegradient3">
                        <ImageView
                            android:id="@+id/Image"
                            android:layout_gravity="center"
                            android:layout_width="23dp"
                            android:layout_height="23dp"
                            android:tint="@color/white"
                            android:src="@drawable/icon_pen_vector"
                            android:gravity="center" />
                    </FrameLayout>
                    <TextView
                        android:layout_below="@+id/EditButton"
                        android:id="@+id/tv_Edit"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/Lbl_EditProfile"
                        android:textAlignment="center"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="3dp"
                        android:textStyle="bold"
                        android:textSize="12sp" />
                </RelativeLayout>
                <RelativeLayout
                    android:layout_marginTop="10dp"
                    android:layout_marginStart="15dp"
                    android:layout_alignParentStart="true"
                    android:layout_toStartOf="@+id/Boost"
                    android:gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <FrameLayout
                        android:id="@+id/SettingsButton"
                        android:clickable="true"
                        android:focusable="true"
                        android:layout_width="55dp"
                        android:layout_height="55dp"
                        android:gravity="center"
                        android:layout_centerHorizontal="true"
                        android:background="@drawable/circlegradient3">
                        <ImageView
                            android:id="@+id/Image"
                            android:layout_gravity="center"
                            android:layout_width="23dp"
                            android:layout_height="23dp"
                            android:tint="@color/white"
                            android:src="@drawable/icon_settings_vector"
                            android:gravity="center" />
                    </FrameLayout>
                    <TextView
                        android:layout_below="@+id/SettingsButton"
                        android:id="@+id/settingstextview"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/Lbl_Settings"
                        android:layout_centerHorizontal="true"
                        android:layout_marginStart="4dp"
                        android:textAlignment="center"
                        android:textStyle="bold"
                        android:layout_marginTop="3dp"
                        android:textSize="12sp" />
                </RelativeLayout>
            </RelativeLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/circleLayouts"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_below="@+id/circleLayouts"
                    android:id="@+id/StFriendsLayout"
                    android:layout_width="match_parent"
                    android:clickable="true"
                    android:background="?attr/selectableItemBackground"
                    android:layout_marginEnd="10dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="15dp"
                    android:padding="5dp"
                    android:gravity="center_vertical"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:id="@+id/StImage5"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:tint="@color/accent"
                        android:src="@drawable/icon_requests_vector"
                        android:layout_marginEnd="10dp"
                        android:layout_marginTop="0dp"
                        android:layout_centerVertical="true"
                        android:layout_alignParentStart="true"
                        android:layout_marginStart="5dp"
                        android:scaleType="centerCrop" />
                    <TextView
                        android:id="@+id/St_5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:layout_toEndOf="@+id/StImage5"
                        android:layout_marginEnd="15dp"
                        android:text="@string/Lbl_Friends"
                        android:gravity="start"
                        android:layout_marginStart="5dp"
                        android:textSize="14sp"
                        android:layout_marginBottom="0dp" />
                    <TextView
                        android:id="@+id/d_5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="?attr/colorSecText"
                        android:layout_toEndOf="@+id/StImage5"
                        android:layout_marginEnd="15dp"
                        android:layout_marginTop="25dp"
                        android:text="@string/Lbl_DisplayAllFriends"
                        android:gravity="start"
                        android:layout_marginStart="5dp"
                        android:textSize="13sp"
                        android:layout_marginBottom="0dp" />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/IlikedLayout"
                    android:layout_below="@+id/StFriendsLayout"
                    android:layout_width="match_parent"
                    android:layout_marginEnd="10dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="15dp"
                    android:padding="5dp"
                    android:clickable="true"
                    android:background="?attr/selectableItemBackground"
                    android:paddingEnd="10dp"
                    android:gravity="center_vertical"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:id="@+id/StImage1"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:tint="@color/accent"
                        android:src="@drawable/icon_i_like_users_vector"
                        android:layout_marginEnd="10dp"
                        android:layout_marginTop="0dp"
                        android:layout_centerVertical="true"
                        android:layout_alignParentStart="true"
                        android:layout_marginStart="5dp"
                        android:scaleType="centerCrop" />
                    <TextView
                        android:id="@+id/St_1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:layout_toEndOf="@+id/StImage1"
                        android:layout_marginEnd="15dp"
                        android:text="@string/Lbl_I_Liked"
                        android:gravity="start"
                        android:layout_marginStart="5dp"
                        android:textSize="14sp"
                        android:layout_marginBottom="0dp" />
                    <TextView
                        android:id="@+id/d_1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="?attr/colorSecText"
                        android:layout_toEndOf="@+id/StImage1"
                        android:layout_marginEnd="15dp"
                        android:layout_marginTop="25dp"
                        android:text="@string/Lbl_I_Liked_desc"
                        android:gravity="start"
                        android:layout_marginStart="5dp"
                        android:textSize="13sp"
                        android:layout_marginBottom="0dp" />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/IDislikedLayout"
                    android:layout_below="@+id/IlikedLayout"
                    android:layout_width="match_parent"
                    android:layout_marginEnd="10dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="15dp"
                    android:padding="5dp"
                    android:clickable="true"
                    android:background="?attr/selectableItemBackground"
                    android:paddingEnd="10dp"
                    android:gravity="center_vertical"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:id="@+id/StImage1"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:tint="@color/accent"
                        android:rotation="180"
                        android:src="@drawable/icon_i_like_vector"
                        android:layout_marginEnd="10dp"
                        android:layout_marginTop="0dp"
                        android:layout_centerVertical="true"
                        android:layout_alignParentStart="true"
                        android:layout_marginStart="5dp"
                        android:scaleType="centerCrop" />
                    <TextView
                        android:id="@+id/St_1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:layout_toEndOf="@+id/StImage1"
                        android:layout_marginEnd="15dp"
                        android:text="@string/Lbl_I_Desliked"
                        android:gravity="start"
                        android:layout_marginStart="5dp"
                        android:textSize="14sp"
                        android:layout_marginBottom="0dp" />
                    <TextView
                        android:id="@+id/d_1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="?attr/colorSecText"
                        android:layout_toEndOf="@+id/StImage1"
                        android:layout_marginEnd="15dp"
                        android:layout_marginTop="25dp"
                        android:text="@string/Lbl_I_Desliked_desc"
                        android:gravity="start"
                        android:layout_marginStart="5dp"
                        android:textSize="13sp"
                        android:layout_marginBottom="0dp" />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/StFavoriteLayout"
                    android:layout_below="@+id/IDislikedLayout"
                    android:layout_width="match_parent"
                    android:layout_marginEnd="10dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="15dp"
                    android:padding="5dp"
                    android:clickable="true"
                    android:background="?attr/selectableItemBackground"
                    android:paddingEnd="10dp"
                    android:gravity="center_vertical"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:id="@+id/StImage1"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:tint="@color/accent"
                        android:src="@drawable/icon_rate_vector"
                        android:layout_marginEnd="10dp"
                        android:layout_marginTop="0dp"
                        android:layout_centerVertical="true"
                        android:layout_alignParentStart="true"
                        android:layout_marginStart="5dp"
                        android:scaleType="centerCrop" />
                    <TextView
                        android:id="@+id/St_1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:layout_toEndOf="@+id/StImage1"
                        android:layout_marginEnd="15dp"
                        android:text="@string/Lbl_Favorite"
                        android:gravity="start"
                        android:layout_marginStart="5dp"
                        android:textSize="14sp"
                        android:layout_marginBottom="0dp" />
                    <TextView
                        android:id="@+id/d_1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="?attr/colorSecText"
                        android:layout_toEndOf="@+id/StImage1"
                        android:layout_marginEnd="15dp"
                        android:layout_marginTop="25dp"
                        android:text="@string/Lbl_DisplayAllFavorite"
                        android:gravity="start"
                        android:layout_marginStart="5dp"
                        android:textSize="13sp"
                        android:layout_marginBottom="0dp" />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/StBlogsLayout"
                    android:layout_below="@+id/StFavoriteLayout"
